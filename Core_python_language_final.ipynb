    "#Width-getter        \n",
    "    @property \n",
    "    def width(self):\n",
    "        print(\"Retrieving the width\")\n",
    "        return self.__width\n",
    "    \n",
    "    @width.setter\n",
    "    def width(self, value):\n",
    "        if value.isdigit():\n",
    "            self.__width = value\n",
    "        else:\n",
    "            print(\"Please only enter numbers for width\")\n",
    "            \n",
    "#Defining Area function or capability\n",
    "\n",
    "    def get_area(self):\n",
