    "class Animal:\n",
    "    def __init__(self, name=\"unknown\", weight=0):\n",
    "        self.__name = name\n",
    "        self.__weight = weight\n",
    " \n",
    "    @property\n",
    "    def name(self, name):\n",
    "        self.__name = name\n",
    " \n",
    "    def make_noise(self):\n",
    "        return \"Grrrrr\"\n",
    " \n",
    "    # Casting to a string\n",
    "    def __str__(self):\n",
    "        return \"{} is a {} and says {}\".format (self.__name, type(self).__name__, self.make_noise())\n",
    " \n",
    "    # Magic methods are used for operator overloading\n",
    "    def __gt__(self, animal2):\n",
    "        if self.__weight > animal2.__weight:\n",
    "            return True\n",
    "        else:\n",
    "            return False\n",
    " \n",
    "#Snake inherits everything from Animal\n",
    "class Snake(Animal):\n",
    "    def __init__(self, name=\"unknown\", owner=\"unknown\", weight=0):\n",
    "        # Just have the super class handle all the initializing\n",
    "        Animal.__init__(self, name, weight)\n",
    "        self.__owner = owner\n",
    " \n",
    "    # Overwrite strength\n",
    "    def __str__(self):\n",
    "        # How to call super class methods\n",
    "        return super().__str__() + \" and is owned by \" + \\\n",
    "        self.__owner\n",
    " \n",
    "animal = Animal(\"π-thon\", 3)\n",
    "print(animal)\n",
    " \n",
    "snake = Snake(\"Jake\", \"Hunter\", 27)\n",
    "print(snake)\n",
    " \n",
    "# Test the magic method\n",
    "print(animal > snake)\n",
    " "
